<h2>New book</h2>
<%= form_with model: @books, url: '/books',method: :post do |f| %>
<h2>title</h2>
<%= f.text_field :title %>

<h2>body</h2>
<%= f.text_area :body %>

<%= f.submit :'Create Book' %>

<% end %>
